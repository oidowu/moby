{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "PostgresDB": {
    "url": "${DATABASE_URL}",
    "name": "PostgresDB",
    "connector": "postgresql",
    "ssl": true
  },
  "neo": {
    "name": "neo",
    "connector": "neo4j-graph",
    "port": "7474",
